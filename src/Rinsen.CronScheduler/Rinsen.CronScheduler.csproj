<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <Title>Rinsen Cron Scheduler</Title>
    <Authors>Fredrik Rinsén</Authors>
    <Description>Cron expression based scheduler for scheduling tasks</Description>
    <Copyright>© Fredrik Rinsén</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>Rinsen</PackageTags>
    <RepositoryUrl>https://github.com/Rinsen/CronScheduler</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.1.1" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="3.0.0" />
  </ItemGroup>

</Project>
